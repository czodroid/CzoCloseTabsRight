{
  "manifest_version": 2,
  "name": "Czo Close Tabs Right",
  "version": "1.2.5",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/czodroid/CzoCloseTabsRight",

  "browser_specific_settings": {
    "gecko": {
      "data_collection_permissions": {
        "required": ["none"]
      },
      "id": "CzoCloseTabsRight@czo",
      "strict_min_version": "78.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["contextMenus"],

  "default_locale": "en"
}
